{
  "enabledManagers": ["dockerfile", "ansible-galaxy", "terraform", "pip_requirements", "github-actions", "regex"],
  "regexManagers": [
    {
      "fileMatch": ["(^|/)*\\.tf$"],
      "matchStrings": [
        "\\s*name\\s*=\\s*\"(?<depName>.*?):(?<currentValue>.*?)\"\\s*#\\s*renovate\\n"
      ],
      "datasourceTemplate": "docker"
    }
  ]
}

{
  "enabledManagers": ["dockerfile", "ansible-galaxy", "terraform", "pip_requirements", "github-actions", "regex"],
  "regexManagers": [
    {
      "fileMatch": ["(^|/)*\\.tf$"],
      "matchStrings": [
        "data\\s\"docker_registry_image\"\\s\".*\"\\s?{\\n\\s*name\\s*=\\s*\"(?<depName>.*?):(?<currentValue>.*?)\""
      ],
      "datasourceTemplate": "docker"
    }
  ]
}
